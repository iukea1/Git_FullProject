    <validationRules>
        <fullName>Check_Prepared_For_belonging_to_account</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK( PreparedFor__c )),
Reseller_AccountId__c!= PreparedFor__r.AccountId
)</errorConditionFormula>
        <errorDisplayField>PreparedFor__c</errorDisplayField>
        <errorMessage>Prepared For doesn&apos;t belong to the Tier1 or, Tier 2 or Tier 3 account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Renew_Quotes_can_be_create_fr_spec_prof</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISNEW(),
OR( 
ISPICKVAL(Opportunity__r.Type,&quot;Support Renewal&quot;),
ISPICKVAL(Opportunity__r.Type,&quot;Subscription Renewal&quot;),
ISPICKVAL(Opportunity__r.Type,&quot;EC Renewal&quot;)
),
NOT($Profile.Name ==&quot;1.5- Sales Power User&quot; ),
NOT($Profile.Name ==&quot;1.7- Renewal Specialist&quot; ),
NOT($Profile.Name ==&quot;5-SP Contract Mgr&quot; )
)</errorConditionFormula>
        <errorMessage>You are not allowed to create quote on renewal opportunity. Please contact Finance or renewal specialist team.</errorMessage>
    </validationRules>
