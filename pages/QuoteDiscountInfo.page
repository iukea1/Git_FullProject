            <td  class="dataClass">
                <apex:outputText value="{0,number,0}%">
                    <apex:param value="{!DiscountQuote.RP_RegServiceDiscount}" />
                </apex:outputText> 
            </td> 
        </tr>
    </table>
    <br/><br/>
    <table width="100%"  border="0" cellpadding="0" cellspacing="0" >
        <tr>
            <td colspan="4">
                <h4>
                     Purchasing Partner Discounts
                    <br/><br/>
                </h4>          
